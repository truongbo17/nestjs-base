    "nest-winston": "^1.9.7",
    "nestjs-command": "^3.1.4",
    "nestjs-console": "^9.0.0",
    "nestjs-i18n": "^10.5.0",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.1",
